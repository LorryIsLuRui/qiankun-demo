{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "name": "qiankun-lerna-demo",
  "version": "1.0.0",
  "dependencies": {
    "qiankun": "^2.10.16"
  },
  "devDependencies": {
    "lerna": "^9.0.0"
  },
  "scripts": {
    "build": "webpack --config webpack.dev.config.js",
    "watch": "webpack --config webpack.dev.config.js --watch",
    "start:main": "lerna run start --scope main-app",
    "start:micro": "lerna run start --scope \"@your-org/micro-*\"",
    "start-1": "npm run start:main && npm run start:micro",
    "start": "lerna run start --parallel"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
